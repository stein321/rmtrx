<resources>

    <!-- Root styles that vary by API level -->
    <style name="FrameworkRoot.Theme.Light" parent="@android:Theme.Holo.Light.DarkActionBar">
        <!-- API 17+ (compatibility) -->
        <item name="listPreferredItemPaddingStart">@dimen/compat_list_preferred_item_padding_start</item>
        <item name="listPreferredItemPaddingEnd">@dimen/compat_list_preferred_item_padding_end</item>
    </style>

    <!-- Immediate parent theme to specify base values for custom attributes -->
    <style name="Theme.Base.Light" parent="FrameworkRoot.Theme.Light">
        <item name="expandableListParentPaddingBottom">@dimen/expandable_list_parent_padding_bottom</item>
        <item name="expandableListParentPaddingTop">@dimen/expandable_list_parent_padding_top</item>

    </style>

    <style name="Theme.rmtrx.Light" parent="Theme.Base.Light"/>

</resources>
